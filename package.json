{
  "name": "gapminder-offline",
  "version": "4.0.1",
  "description": "Gapminder Tools Offline",
  "author": "Gapminder <ola@gapminder.org>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gapminder/gapminder-offline.git"
  },
  "license": "GPL-3.0",
  "keywords": [
    "angular",
    "angular 6",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "googleAnalyticsId": "UA-67908993-4",
  "private": true,
  "scripts": {
    "postinstall": "npx electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "npm run electron:tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -o",
    "electron:tsc": "tsc --typeRoots [\\\"node_modules/@types\\\"] main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:dev && electron .",
    "electron:linux": "npm run build:dev && npx electron-builder build --linux && ./scripts/additional-copy-linux",
    "electron:windows": "npm run build:dev && npx electron-builder build --windows",
    "electron:windows32": "npm run build:dev && npx electron-builder build --windows --ia32",
    "electron:mac": "npm run build:dev && npx electron-builder build --mac && ./scripts/additional-copy-mac",
    "package-dmg": "node package-dmg.js",
    "start-webdriver": "node node_modules/electron-chromedriver/chromedriver &",
    "e2e-test": "./node_modules/.bin/protractor ./e2e/protractor.conf.js",
    "e2e:linux": "npm run start-webdriver && npm run e2e-test",
    "e2e:windows": "start npm run start-webdriver && npm run e2e-test",
    "update-webdriver": "webdriver-manager update",
    "test": "ng test",
    "e2e": "ng e2e",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "conventional-github-releaser -p angular",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push origin development && git push --tags && npm run github-release"
  },
  "dependencies": {
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "async": "2.6.1",
    "async-waterfall": "0.1.5",
    "csv-parse": "2.5.0",
    "ddf-validation": "1.19.0",
    "electron-easy-updater": "0.7.1",
    "folder-hash": "2.1.1",
    "fs-extra": "7.0.0",
    "git-fetch-pack": "0.1.1",
    "git-transport-protocol": "0.1.0",
    "is-online": "7.0.0",
    "lodash": "4.17.10",
    "minimist": "1.2.0",
    "ps-node": "0.1.6",
    "semver": "5.5.0",
    "semver-sort": "0.0.4",
    "tslib": "1.9.3",
    "universal-analytics": "0.4.17",
    "urlon": "3.0.1",
    "vizabi-csv-reader": "0.4.0",
    "vizabi-ddfcsv-reader": "2.4.3",
    "vizabi-excel-reader": "0.4.1",
    "zip-dir": "1.0.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/common": "6.1.8",
    "@angular/compiler": "6.1.8",
    "@angular/compiler-cli": "6.1.8",
    "@angular/core": "6.1.8",
    "@angular/forms": "6.1.8",
    "@angular/http": "6.1.8",
    "@angular/language-service": "6.1.8",
    "@angular/platform-browser": "6.1.8",
    "@angular/platform-browser-dynamic": "6.1.8",
    "@angular/router": "6.1.8",
    "@types/core-js": "2.5.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.5.2",
    "codelyzer": "4.4.2",
    "compare-version": "0.1.2",
    "conventional-changelog": "1.1.3",
    "conventional-changelog-cli": "1.3.1",
    "conventional-github-releaser": "1.1.9",
    "core-js": "2.5.7",
    "electron": "2.0.5",
    "electron-builder": "20.24.4",
    "electron-chromedriver": "2.0.0",
    "electron-reload": "1.2.5",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.2.0",
    "ngx-bootstrap": "3.0.1",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "protractor": "5.3.2",
    "rxjs": "6.2.2",
    "rxjs-compat": "6.2.2",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "2.7.2",
    "vizabi": "0.38.5",
    "vizabi-barrankchart": "2.4.1",
    "vizabi-bubblechart": "2.3.0",
    "vizabi-bubblemap": "2.3.0",
    "vizabi-config-systema_globalis": "2.9.1",
    "vizabi-linechart": "2.3.0",
    "vizabi-mountainchart": "2.4.0",
    "wait-on": "2.1.0",
    "webdriver-manager": "12.1.0",
    "zone.js": "0.8.26"
  },
  "optionalDependencies": {
    "appdmg": "0.5.2"
  }
}
